{
    "version": 2,
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/index.js"
      },
      {
        "source": "/static/(.*)",
        "destination": "/frontend/build/static/$1"
      },
      {
        "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg))",
        "destination": "/frontend/build/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/frontend/build/index.html"
      }
    ]
  }
  